# @configure_input@

TOPSRC      = @top_builddir@
CONFIG_DIR  = $(TOPSRC)
SUBDIRS     = lib bin 

all install bare clean realclean ::
	@for i in $(SUBDIRS) ; \
	do echo; \
	  echo '<<<' Making $@ in $$i '>>>'; \
	  cd $$i ; $(MAKE) -s $@; cd ..; \
	done

include $(CONFIG_DIR)/Makedefs
 
include $(CONFIG_DIR)/Makerules

